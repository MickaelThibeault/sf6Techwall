{% extends 'template.html.twig' %}

{% block title %}Todo{% endblock %}

{% block PageTitle %}Liste des todos{% endblock %}

{% block body %}
    <ol class="list-group">
        {% for name,content in app.session.get('todos') %}
           <li class="list-group-item">{{ name }} :  {{ content }}</li>
        {% endfor %}
    </ol>
{% endblock %}
